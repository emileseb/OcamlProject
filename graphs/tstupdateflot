digraph finite_state_machine {

    rankdir=LR;

    size="8,5"

    node [shape = circle];
0 -> 2 [ label = "(8/8)" ];
0 -> 3 [ label = "(10/10)" ];
0 -> 1 [ label = "(7/7)" ];
2 -> 4 [ label = "(12/12)" ];
3 -> 4 [ label = "(5/5)" ];
3 -> 2 [ label = "(2/2)" ];
3 -> 1 [ label = "(11/11)" ];
1 -> 4 [ label = "(1/1)" ];
1 -> 5 [ label = "(21/21)" ];
4 -> 5 [ label = "(14/14)" ];
}