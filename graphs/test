digraph finite_state_machine {

    rankdir=LR;

    size="8,5"

    node [shape = circle];
4 -> 5 [ label = "1" ];
1 -> 5 [ label = "1" ];
1 -> 4 [ label = "1" ];
3 -> 1 [ label = "1" ];
3 -> 2 [ label = "1" ];
3 -> 4 [ label = "1" ];
2 -> 4 [ label = "1" ];
0 -> 1 [ label = "1" ];
0 -> 3 [ label = "1" ];
0 -> 2 [ label = "1" ];
}