digraph finite_state_machine {

    rankdir=LR;

    size="8,5"

    node [shape = circle];
0 -> 2 [ label = "(0/8)" ];
0 -> 3 [ label = "(0/10)" ];
0 -> 1 [ label = "(0/7)" ];
2 -> 4 [ label = "(0/12)" ];
3 -> 4 [ label = "(0/5)" ];
3 -> 2 [ label = "(0/2)" ];
3 -> 1 [ label = "(0/11)" ];
1 -> 4 [ label = "(0/1)" ];
1 -> 5 [ label = "(0/21)" ];
4 -> 5 [ label = "(0/14)" ];
}